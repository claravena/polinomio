#include <iostream>
#include "vector.h"

using namespace std;

int main(){
  vector<double> v1 = {1,1,1,1};
  vector<double> v2 = {1,1,1,1};
  Vector M(4);
  Vector G(4,v1);
  
  M.set_vector(v1);
  G.set_vector(v2);
  //cout <<M<<endl;
  //cout <<G<<endl;
  cout <<M*G<<endl;
  //cout <<2*G<<endl;
  


  
   
  return 0;
}

